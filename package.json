    "dev:remix": "remix dev",
    "dev:components": "start-storybook -p 4001",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "remix-serve build",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
